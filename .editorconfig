root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = true
quote_type = double
trim_trailing_whitespace = true

[*.json]
insert_final_newline = false

[**.bundle.js,**.bundle.js.map]
indent_style = unset
indent_size = unset
insert_final_newline = unset
trim_trailing_whitespace = unset

[*.md]
trim_trailing_whitespace = false
